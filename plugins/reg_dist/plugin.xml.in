<?xml version="1.0" encoding="UTF-8"?>
<plugin id="GOffice_reg_distreg">
	<information>
		<_name>Regression curve : distributions</_name>
		<_description>Distributions fitting curves</_description>
	</information>
	<loader type="Gnumeric_Builtin:module">
		<attribute name="module_file" value="distreg"/>
	</loader>
	<services>
		<service type="trendline_engine" id="GogGaussianCurve">
			<information>
				<_description>Gaussian fitting curve engine</_description>
			</information>
		</service>
		<service type="trendline_type" id="distreg">
			<file>reg-types.xml</file>
			<information>
				<_description>Distributions fitting curves types</_description>
			</information>
		</service>
	</services>
</plugin>
